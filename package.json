{
    "name": "bmw-status",
    "version": "0.3.1",
    "private": true,
    "description": "BMW ConnectedDrive Status",
    "author": "Mihai Blaga <mihaiblaga89@gmail.com> (https://www.mihaiblaga.dev)",
    "dependencies": {
        "@mihaiblaga89/bmw-connecteddrive-api": "^0.5.4",
        "@reach/router": "^1.3.3",
        "electron-is-dev": "^1.1.0",
        "electron-store": "^5.1.1",
        "keytar": "^5.4.0",
        "node-notifier": "^6.0.0",
        "prop-types": "^15.7.2",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "semantic-ui-css": "^2.4.1",
        "semantic-ui-react": "^0.88.2",
        "styled-components": "^5.0.1",
        "typeface-roboto": "^0.0.75"
    },
    "scripts": {
        "postinstall": "electron-builder install-app-deps",
        "electron-pack": "build -mw",
        "react-start": "node scripts/start.js",
        "react-build": "node scripts/build.js",
        "react-test": "node scripts/test.js --env=jsdom",
        "react-eject": "node scripts/eject.js",
        "publish": "yarn react-build && electron-builder --publish=always",
        "build": "yarn react-build && electron-builder",
        "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\""
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/core": "7.9.0",
        "@svgr/webpack": "4.3.3",
        "babel-eslint": "10.1.0",
        "babel-jest": "^24.9.0",
        "babel-loader": "8.1.0",
        "babel-plugin-named-asset-import": "^0.3.6",
        "babel-plugin-styled-components": "^1.10.7",
        "babel-preset-react-app": "^9.1.2",
        "camelcase": "^5.3.1",
        "case-sensitive-paths-webpack-plugin": "2.3.0",
        "concurrently": "^5.1.0",
        "cross-env": "^7.0.2",
        "css-loader": "2.1.1",
        "dotenv": "8.2.0",
        "dotenv-expand": "5.1.0",
        "electron": "^7.1.14",
        "electron-builder": "^21.2.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-config-react-app": "^5.2.0",
        "eslint-loader": "3.0.3",
        "eslint-plugin-flowtype": "4.6.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jest": "^23.8.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^2.5.0",
        "file-loader": "3.0.1",
        "fs-extra": "8.1.0",
        "html-webpack-plugin": "4.0.0-beta.14",
        "husky": "^4.2.3",
        "identity-obj-proxy": "3.0.0",
        "is-wsl": "^2.1.1",
        "jest": "^24.9.0",
        "jest-environment-jsdom-fourteen": "1.0.1",
        "jest-watch-typeahead": "0.4.2",
        "mini-css-extract-plugin": "0.9.0",
        "optimize-css-assets-webpack-plugin": "5.0.3",
        "pnp-webpack-plugin": "1.6.4",
        "postcss-flexbugs-fixes": "4.2.0",
        "postcss-loader": "3.0.0",
        "postcss-normalize": "8.0.1",
        "postcss-preset-env": "6.7.0",
        "postcss-safe-parser": "4.0.2",
        "prettier": "^1.19.1",
        "pretty-quick": "^2.0.1",
        "react-app-polyfill": "^1.0.6",
        "react-dev-utils": "^10.2.0",
        "resolve": "1.15.1",
        "resolve-url-loader": "3.1.1",
        "sass-loader": "7.3.1",
        "style-loader": "1.1.3",
        "terser-webpack-plugin": "1.4.3",
        "url-loader": "3.0.0",
        "wait-on": "^3.3.0",
        "webpack": "4.42.0",
        "webpack-dev-server": "3.10.3",
        "webpack-manifest-plugin": "2.2.0",
        "workbox-webpack-plugin": "4.3.1"
    },
    "homepage": "./",
    "build": {
        "directories": {
            "buildResources": "public"
        },
        "appId": "dev.mihaiblaga89.bmwstatus",
        "productName": "BMWStatus",
        "copyright": "Copyright Â© 2019 ${author}",
        "mac": {
            "category": "public.app-category.utilities",
            "icon": "./public/resources/icons/mac/app.icns"
        },
        "asarUnpack": [
            "./node_modules/node-notifier/vendor/**"
        ],
        "extraResources": [
            {
                "from": "./node_modules/node-notifier/vendor",
                "to": "app.asar.unpacked/node_modules/node-notifier/vendor"
            }
        ]
    },
    "main": "build/electron.js",
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "jest": {
        "roots": [
            "<rootDir>/src"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "setupFilesAfterEnv": [],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jest-environment-jsdom-fourteen",
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "modulePaths": [],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ]
    },
    "babel": {
        "presets": [
            "react-app"
        ],
        "plugins": [
            "babel-plugin-styled-components"
        ]
    }
}
